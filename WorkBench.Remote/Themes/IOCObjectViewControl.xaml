        </GroupBox>-->
        <!--<GroupBox Grid.Row="1" Header="实例视图" Margin="5">
            <ListBox x:Name="DependenciesListBox" Background="#E3FAE9"/>
        </GroupBox>-->
        <!--<GroupBox Grid.Row="3" Header="未完成注入的实例" Margin="5">
            <ListBox x:Name="UnfinishedDependenciesListBox" Background="#FFE9D7"/>
        </GroupBox>-->
    </Grid>
